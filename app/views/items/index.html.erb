<h1>Item List</h1>
<table>
	<tr>
		<td>Name</td> <td>Username</td> <td>Description</td> <td>Price</td>
	</tr>
		<% @items.each do |p| %>
	<tr>
		<td><%= link_to p.name, item_path(p)%></td> <td><%=p.username%></td> <td><%=p.price%></td> <td><%=p.description%></td>
<%= link_to "Delete", destroy_item_path(p)%>

		
	</tr>
<% end %>
</table>
